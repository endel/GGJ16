@font-face
  font-family 'Berry_Rotunda'
  src url('../fonts/Berry_Rotunda.eot')
  src url('../fonts/Berry_Rotunda.eot?#iefix') format('embedded-opentype'),
       url('../fonts/Berry_Rotunda.woff') format('woff'),
       url('../fonts/Berry_Rotunda.ttf') format('truetype'),
       url('../fonts/Berry_Rotunda.svg#Berry_Rotunda.svg') format('svg')
  font-weight normal
  font-style normal

p#font-rotunda
  position absolute
  font-family 'Berry_Rotunda'
  visibility hidden
